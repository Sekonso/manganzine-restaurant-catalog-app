{
  "extends": "eslint-config-airbnb-base",
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-restricted-globals": "off",
    "linebreak-style": "off",
    "no-useless-constructor": "off",
    "quotes": ["error", "double"],
    "comma-dangle": ["error", "always-multiline"]
  }
}
